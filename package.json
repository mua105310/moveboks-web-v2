{
  "name": "moveboks-web-v2",
  "version": "v0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "buildprod": "npm run version:patch && cross-env NODE_ENV=production npm run build",
    "buildlocal": "cross-env NODE_ENV=local npm run build",
    "version:patch": "npm --no-git-tag-version version patch && node -e \"const fs=require('fs');const pkg=require('./package.json');pkg.version='v'+pkg.version;fs.writeFileSync('package.json',JSON.stringify(pkg,null,2))\"",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^6.4.1",
    "embla-carousel-autoplay": "^8.3.0",
    "embla-carousel-react": "^8.3.0",
    "lucide-react": "^0.474.0",
    "next": "^15.1.6",
    "primeicons": "^7.0.0",
    "primereact": "^10.8.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-icons": "^5.3.0",
    "swiper": "^11.1.14"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "cross-env": "^7.0.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
